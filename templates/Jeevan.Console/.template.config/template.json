{
    "$schema": "http://json.schemastore.org/template",
    "author": "Jeevan James",
    "classifications": [ "Jeevan", "Console" ],
    "name": "Jeevan Console",
    "identity": "Jeevan.Console.CSharp",
    "groupIdentity": "Jeevan.Console",
    "shortName": "jeevan-console",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "sourceName": "Jeevan.Console",
    "preferNameDirectory": true,
    "symbols": {
        "relativens": {
            "type": "parameter",
            "isRequired": true,
            "replaces": "SymbolNamespace",
            "description": "The namespace of this project, relative to the base namespace specified in the Directory.Build.props file."
        },
        "description": {
            "type": "parameter",
            "isRequired": false,
            "replaces": "SymbolDescription",
            "defaultValue": "Console application",
            "description": "Description of the console application."
        }
    }
}
