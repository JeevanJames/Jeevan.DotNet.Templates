{
    "$schema": "http://json.schemastore.org/template",
    "author": "Jeevan James",
    "classifications": [ "Jeevan", "Console", "Tool" ],
    "name": "Jeevan Console Tool",
    "identity": "Jeevan.Console.Tool.CSharp",
    "groupIdentity": "Jeevan.Console.Tool",
    "shortName": "jeevan-console-tool",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "sourceName": "Jeevan.Console.Tool",
    "preferNameDirectory": true,
    "symbols": {
        "relativens": {
            "type": "parameter",
            "isRequired": true,
            "replaces": "SymbolNamespace",
            "description": "The namespace of this project, relative to the base namespace specified in the Directory.Build.props file."
        },
        "tool-name": {
            "type": "parameter",
            "isRequired": true,
            "replaces": "SymbolToolName",
            "description": "The name of the tool."
        },
        "description": {
            "type": "parameter",
            "isRequired": false,
            "replaces": "SymbolDescription",
            "defaultValue": ".NET Core tool console application",
            "description": "Description of the console tool application."
        },
        "framework": {
            "type": "parameter",
            "isRequired": false,
            "replaces": "SymbolFramework",
            "defaultValue": "net6.0",
            "description": "Target framework of the console tool application."
        }
    }
}
