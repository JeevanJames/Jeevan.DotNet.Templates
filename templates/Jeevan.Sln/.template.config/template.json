{
    "$schema": "http://json.schemastore.org/template",
    "author": "Jeevan James",
    "classifications": [ "Jeevan", "Solution" ],
    "name": "Jeevan Solution",
    "identity": "Jeevan Solution",
    "groupIdentity": "Jeevan.Solution",
    "shortName": "jeevan-sln",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "sourceName": "Jeevan.Sln",
    "preferNameDirectory": true,
    "symbols": {
        "basens": {
            "type": "parameter",
            "isRequired": true,
            "replaces": "SymbolNamespace",
            "description": "The base namespace for all projects in this solution."
        },
        "product": {
            "type": "parameter",
            "isRequired": true,
            "replaces": "SymbolProduct",
            "description": "The descriptive product name for this solution."
        },
        "repository": {
            "type": "parameter",
            "isRequired": true,
            "replaces": "SymbolRepository",
            "description": "The repository name for this solution."
        },
        "copyright-years": {
            "type": "parameter",
            "isRequired": false,
            "replaces": "SymbolCopyrightYears",
            "defaultValue": "2023",
            "description": "The copyright year or years to apply to various files."
        }
    }
}
